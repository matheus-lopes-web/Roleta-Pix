{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/enviar-resultado",
      "dest": "/api/enviar-resultado.js"
    },
    {
      "src": "/.*\\.(js|css|html|png|jpg|gif)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}